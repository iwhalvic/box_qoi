<prototype>
  <common>
	<!-- Setup System-->
    <problem_type>neutronics</problem_type>
    <spatial_method>sdm_pwld</spatial_method>
    <fem_type>fem_lump</fem_type>
    <geometry>xyz</geometry>
	<!-- Use a single Energy group with 3 spacial dimmensions-->
    <ngroups>1</ngroups>
    <eg_floor>0.0</eg_floor>
    <dimensions>3</dimensions>
    <iscat>0</iscat>
    <diagnostics_level>3</diagnostics_level>
	<!-- Ignore Upscattering -->
    <no_upscattering>true</no_upscattering>
	<!-- Set Tolerances -->
    <wgs_iterative_method>gmres</wgs_iterative_method>
    <wgs_gmres_restart_value>5</wgs_gmres_restart_value>
    <wgs_residual_tolerance>1.0e-8</wgs_residual_tolerance>
    <wgs_residual_max_its>1000</wgs_residual_max_its>
    <wgs_pointwise_tolerance>1.0e-2</wgs_pointwise_tolerance>
    <wgs_pointwise_max_its>1</wgs_pointwise_max_its>
    <ags_iterative_method>richardson</ags_iterative_method>
    <ags_residual_tolerance>1.0e-8</ags_residual_tolerance>
    <ags_residual_max_its>1000</ags_residual_max_its>
    <ags_pointwise_tolerance>1.0e-2</ags_pointwise_tolerance>
    <ags_pointwise_max_its>1</ags_pointwise_max_its>
	<!-- Some Partition Stuff -->
    <partition_type>manual</partition_type>
    <partition_params>
      <partition_x>1</partition_x>
      <partition_y>1</partition_y>
      <partition_z>1</partition_z>
    </partition_params>
  </common>
  <!-- Energy Groups-->
    <groupsets>
    <energy_set>
      <ID>0</ID>
	  <set_include>1</set_include>
      <quad_info>
        <quad_plevel>2</quad_plevel>
        <quad_azimuthal>2</quad_azimuthal>
        <quad_norm>12.566370614</quad_norm>
        <quad_type>productgausslegcheb</quad_type>
      </quad_info>
      <angle_set_aggregation>polar</angle_set_aggregation>
    </energy_set>
  </groupsets>
  <!-- Define system as 30x30x30 cube-->
  <dimension>
	<dimension.id>0</dimension.id>
		<dim.division>
			<dim.division.cells>10</dim.division.cells>
			<dim.division.start>0.0</dim.division.start>
			<dim.division.end>10.0</dim.division.end>
			<dim.division.id>1.1</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>4</dim.division.cells>
			<dim.division.start>10.0</dim.division.start>
			<dim.division.end>14.0</dim.division.end>
			<dim.division.id>1.2</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>2</dim.division.cells>
			<dim.division.start>14.0</dim.division.start>
			<dim.division.end>16.0</dim.division.end>
			<dim.division.id>1.3</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>4</dim.division.cells>
			<dim.division.start>16.0</dim.division.start>
			<dim.division.end>20.0</dim.division.end>
			<dim.division.id>1.4</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>10</dim.division.cells>
			<dim.division.start>20.0</dim.division.start>
			<dim.division.end>30.0</dim.division.end>
			<dim.division.id>1.5</dim.division.id>
		</dim.division>
	<dimension.id>1</dimension.id>
		<dim.division>
			<dim.division.cells>10</dim.division.cells>
			<dim.division.start>0.0</dim.division.start>
			<dim.division.end>10.0</dim.division.end>
			<dim.division.id>2.1</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>4</dim.division.cells>
			<dim.division.start>10.0</dim.division.start>
			<dim.division.end>14.0</dim.division.end>
			<dim.division.id>2.2</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>2</dim.division.cells>
			<dim.division.start>14.0</dim.division.start>
			<dim.division.end>16.0</dim.division.end>
			<dim.division.id>2.3</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>4</dim.division.cells>
			<dim.division.start>16.0</dim.division.start>
			<dim.division.end>20.0</dim.division.end>
			<dim.division.id>2.4</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division>
			<dim.division.cells>10</dim.division.cells>
			<dim.division.start>20.0</dim.division.start>
			<dim.division.end>30.0</dim.division.end>
			<dim.division.id>2.5</dim.division.id>
		</dim.division>
	<dimension.id>2</dimension.id>
		<dim.division>
			<dim.division.cells>10</dim.division.cells>
			<dim.division.start>0.0</dim.division.start>
			<dim.division.end>10.0</dim.division.end>
			<dim.division.id>3.1</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>4</dim.division.cells>
			<dim.division.start>10.0</dim.division.start>
			<dim.division.end>14.0</dim.division.end>
			<dim.division.id>3.2</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>2</dim.division.cells>
			<dim.division.start>14.0</dim.division.start>
			<dim.division.end>16.0</dim.division.end>
			<dim.division.id>3.3</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>4</dim.division.cells>
			<dim.division.start>16.0</dim.division.start>
			<dim.division.end>20.0</dim.division.end>
			<dim.division.id>3.4</dim.division.id>
		</dim.division>
		<dim.division>
			<dim.division.cells>10</dim.division.cells>
			<dim.division.start>20.0</dim.division.start>
			<dim.division.end>30.0</dim.division.end>
			<dim.division.id>3.5</dim.division.id>
		</dim.division>
  </dimension>
  <regions>
    <regions-edit_region>
      <edit_reg.id>detector</edit_reg.id>
      <edit_reg.edit>rxrate_abs</edit_reg.edit>
    </regions-edit_region>
  </regions>
  <edit_region_material>
   <material>BF3</material>
   <edit_region>detector</edit_region>
  </edit_region_material>
  <component_def>
  <!--material definitions, AmBe source, BF3 detector -->
  <material_def>
    <material_def.name>AmBe</material_def.name>
    <material_def.component>
      <material_def.component.id>1</material_def.component.id>
      <material_def.component.density>1.1649E-3</material_def.component.density> 
    </material_def.component>
  </material_def>
  <material_def>
    <material_def.name>BF3</material_def.name>
    <material_def.component>
      <material_def.component.id>C</material_def.component.id>
      <material_def.component.density>6.5138E-6</material_def.component.density>
  </material_def>
  <!-- Define AmBe  point source in center of system-->
  <source_material>
    <source>AmBe</source>
    <material>AmBe</material>
  </source_material>
     <named_sources>
    <source_def>
      <source_def_name>AmBe</source_def_name>
      <intensity>12.566370614</intensity>
    </source_def>
  </named_sources>
  <source_geometry>
    <source_region>
      <source_name>AmBe</source_name>
      <source_dim_bounds>
        <source_dim_bounds_dim>0</source_dim_bounds_dim>
        <source_dim_start>3</source_dim_start>
        <source_dim_end>3</source_dim_end>
      </source_dim_bounds>
      <source_dim_bounds>
        <source_dim_bounds_dim>1</source_dim_bounds_dim>
        <source_dim_start>3</source_dim_start>
        <source_dim_end>3</source_dim_end>
      </source_dim_bounds>
      <source_dim_bounds>
        <source_dim_bounds_dim>2</source_dim_bounds_dim>
        <source_dim_start>3</source_dim_start>
        <source_dim_end>3</source_dim_end>
      </source_dim_bounds>
    </source_region>
  </source_geometry>
  <!-- Boundry Definitions -->
  <boundary_info>
    <y_min_bound>
      <bound_type>vacuum</bound_type>
    </y_min_bound>
    <y_max_bound>
      <bound_type>vacuum</bound_type>
    </y_max_bound>
    <x_min_bound>
      <bound_type>vacuum</bound_type>
    </x_min_bound>
    <x_max_bound>
      <bound_type>vacuum</bound_type>
    </x_max_bound>
    <z_min_bound>
      <bound_type>vacuum</bound_type>
    </z_min_bound>
    <z_max_bound>
      <bound_type>vacuum</bound_type>
    </z_max_bound>
  </boundary_info>
  <!-- define BF3 detector/QoI, 2x2x2 centered in top cube -->
  <QOI_def>
    <QOI_type>detector_response</QOI_type>
    <QOI_id>detecrr</QOI_id>
    <keyComp>BF3</keyComp>
    <QOI_reg.dim_bounds>
      <QOI_reg.dim_bounds.dim>0</QOI_reg.dim_bounds.dim>
      <QOI_reg.dim_bounds.div_start>14</QOI_reg.dim_bounds.div_start>
      <QOI_reg.dim_bounds.div_end>16</QOI_reg.dim_bounds.div_end>
    </QOI_reg.dim_bounds>
    <QOI_reg.dim_bounds>
      <QOI_reg.dim_bounds.dim>1</QOI_reg.dim_bounds.dim>
      <QOI_reg.dim_bounds.div_start>14</QOI_reg.dim_bounds.div_start>
      <QOI_reg.dim_bounds.div_end>16</QOI_reg.dim_bounds.div_end>
    </QOI_reg.dim_bounds>
    <QOI_reg.dim_bounds>
      <QOI_reg.dim_bounds.dim>2</QOI_reg.dim_bounds.dim>
      <QOI_reg.dim_bounds.div_start>4</QOI_reg.dim_bounds.div_start>
      <QOI_reg.dim_bounds.div_end>6</QOI_reg.dim_bounds.div_end>
    </QOI_reg.dim_bounds>
  </QOI_def>
</prototype>
